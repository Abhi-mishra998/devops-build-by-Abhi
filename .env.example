# Docker Hub Configuration
DOCKERHUB_USERNAME=your-dockerhub-username
DOCKERHUB_DEV_REPO=your-username/dev
DOCKERHUB_PROD_REPO=your-username/prod
DOCKERHUB_TOKEN=your-dockerhub-access-token

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
EC2_INSTANCE_ID=i-xxxxxxxxxxxxxxxxx
EC2_PUBLIC_IP=xx.xx.xx.xx
EC2_KEY_PATH=/path/to/your-key.pem
EC2_USER=ec2-user

# GitHub Configuration
GITHUB_REPO=https://github.com/sriram-R-krishnan/devops-build
GITHUB_TOKEN=your-github-personal-access-token
GITHUB_USERNAME=your-github-username

# Jenkins Configuration
JENKINS_URL=http://localhost:8080
JENKINS_USER=admin
JENKINS_TOKEN=your-jenkins-api-token

# Monitoring Configuration
MONITORING_URL=http://localhost:3001
NOTIFICATION_EMAIL=your-email@example.com
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/WEBHOOK

# Application Configuration
APP_NAME=devops-react-app
APP_PORT=80
NODE_ENV=production

# Build Configuration
BUILD_VERSION=1.0.0
BUILD_NUMBER=1
